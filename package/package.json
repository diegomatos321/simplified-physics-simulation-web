{
    "name": "@devdiegomatos/liso-engine",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
